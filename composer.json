{
  "name": "ytake/prototype.container",
  "description": "dependency injection / service container",
  "minimum-stability": "dev",
  "license": "MIT",
  "keywords": [
    "container",
    "field injection",
    "reflection",
    "annotation",
    "dependency injection"
  ],
  "authors": [
    {
      "name": "yuuki.takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=5.5",
    "nikic/php-parser": "1.*",
    "classpreloader/classpreloader": "1.*",
    "doctrine/annotations": "1.*",
    "doctrine/common": "2.*",
    "symfony/finder": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "satooshi/php-coveralls": "dev-master",
    "facebook/xhprof": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Iono\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iono\\_TestContainer\\": [
        "tests/"
      ]
    }
  }
}